<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CamelTextView">
        <attr name="tvPrefixText" format="string"/>
        <attr name="tvPrefixTextColor" format="color"/>
        <attr name="tvPrefixTextSize" format="dimension"/>
        <attr name="tvPrefixTextFont" format="reference"/>
        <attr name="tvPrefixTextStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>

        <attr name="tvSuffixText" format="string"/>
        <attr name="tvSuffixTextColor" format="color"/>
        <attr name="tvSuffixTextSize" format="dimension"/>
        <attr name="tvSuffixTextFont" format="reference"/>
        <attr name="tvSuffixTextStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
    </declare-styleable>
</resources>